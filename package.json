{
  "name": "vibrant-solutions",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "husky": "^4.3.8",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.2.0",
    "react-responsive-carousel": "^3.2.23",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss}": [
      "prettier --write --config .prettierrc.json"
    ],
    "*.json": [
      "prettier --write --single-quote=false --config .prettierrc.json"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "react-router-dom": "^6.23.1",
    "style-loader": "^4.0.0"
  }
}
